{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "esnext"
    ],
    "jsx": "react-native",
    "module": "commonjs",
    "resolveJsonModule": true,
    "allowJs": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "skipLibCheck": true
  },
  "extends": "expo/tsconfig.base"
}
